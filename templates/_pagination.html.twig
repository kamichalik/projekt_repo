{% block body %}

<div class="pagination" style="color: green !important">
    {% if pageNumber > 1 %}
    <li class="page-item"><a class="page-link" href=" {{ url(path, {'pageNumber': pageNumber - 1}) }} "aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
            <span class="sr-only">Previous</span>
        </a>
    </li>
    {% endif %}
    <li class="page-item"><a class="page-link" href=" {{ url(path, {'pageNumber': pageNumber + 1}) }} "aria-label="Next">
            <span aria-hidden="true">&raquo;</span>
            <span class="sr-only">Previous</span>
        </a>
    </li>

</div>

{% endblock %}
